<div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1" class=""></li>
        <li data-target="#myCarousel" data-slide-to="2" class=""></li>
        <li data-target="#myCarousel" data-slide-to="3" class=""></li>
      </ol>
      <!-- wrapper for slides -->
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <img class="first-slide" src="<%=asset_path("musician.png")%>" alt="Musician with Guitar">
          <div class="container">
            <div class="carousel-caption">
              <h1>Example headline.</h1>
              <p>Sample Text.</p>
              <p><a class="btn btn-lg btn-primary" href="#" role="button">Sign up today</a></p>
            </div>
          </div>
        </div>
        <div class="item">
          <img class="second-slide" src="<%=asset_path("live-band.png")%>" alt="Live Band">
          <div class="container">
            <div class="carousel-caption">
              <h1>Example headline.</h1>
              <p>Sample Text.</p>
              <p><a class="btn btn-lg btn-primary" href="#" role="button">Learn more</a></p>
            </div>
          </div>
        </div>
        <div class="item">
          <img class="third-slide" src="<%=asset_path("piano-solo.png")%>" alt="Piano Solo">
          <div class="container">
            <div class="carousel-caption">
              <h1>Example headline.</h1>
              <p>Sample Text.</p>
              <p><a class="btn btn-lg btn-primary" href="#" role="button">Browse gallery</a></p>
            </div>
          </div>
        </div>
        <div class="item">
          <img class="fourth-slide" src="<%=asset_path("instruments.png")%>" alt="Instruments in the Air">
          <div class="container">
            <div class="carousel-caption">
              <h1>Example headline.</h1>
              <p>Sample Text.</p>
              <p><a class="btn btn-lg btn-primary" href="#" role="button">Browse gallery</a></p>
            </div>
          </div>
        </div>
      </div>
      <!-- controls or next and prev buttons -->
      <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
</div>



